{
    "id": "com.github.lugamuga.mattermost-yandex-calendar-plugin",
    "name": "Yandex Calendar",
    "description": "Yandex Calendar plugin for Mattermost (CALDav)",
    "homepage_url": "https://github.com/lugamuga/mattermost-yandex-calendar-plugin",
    "support_url": "https://github.com/lugamuga/mattermost-yandex-calendar-plugin/issues",
    "release_notes_url": "https://github.com/lugamuga/mattermost-yandex-calendar-plugin/releases/tag/v0.7.1",
    "icon_path": "assets/icon.svg",
    "version": "0.7.1",
    "min_server_version": "5.10.0",
    "server": {
        "executables": {
            "linux-amd64": "server/dist/plugin-linux-amd64",
            "darwin-amd64": "server/dist/plugin-darwin-amd64",
            "windows-amd64": "server/dist/plugin-windows-amd64.exe"
        }
    },
    "settings_schema": {
        "header": "Enjoy using this plugin? Want to contribute and make it more awesome? You can [fork the repository](https://github.com/LugaMuga/mattermost-yandex-calendar-plugin) and submit your pull requests!",
        "settings": [
            {
                "key": "ServerUrl",
                "display_name": "CALDav server URL:",
                "type": "text",
                "default": "https://caldav.yandex.ru"
            }
        ]
    }
}
